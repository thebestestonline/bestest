<div class="container-fluid contact-bar">
  <div class="row">
   <% if params[:plan] == '2' %>
      <h2 class="text-center">Sign Up & Get Listed</h2>
    <% else %>
      <h2 class="text-center">Sign Up</h2>
    <% end %>
    <div class="col-md-6 col-md-offset-3">
      <div class="well">
        <% if params[:plan] == '2' %>
          <%= render "pro_form" %>
        <% else %>
          <%= render "basic_form" %>
        <% end %>
        <%= render "devise/shared/links" %>
      </div>
    </div>
  </div>
</div>
